{
    "builds": [
      {
        "src": "./index.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
        {
            "src": "/(.*)", 
            "dest": "/index.js"
        }
    ],
    "crons": [
    {
      "path": "/src/Utils/dollarUpdate.js",
      "schedule": "0 13 * * *"
    }
  ]
}
